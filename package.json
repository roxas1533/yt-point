{
  "name": "yt-point",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "bunx --bun vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "sidecar:build": "cd sidecar && bun run build",
    "lint": "biome check . && cargo clippy --manifest-path src-tauri/Cargo.toml",
